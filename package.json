{
  "name": "valuex-x-demo",
  "version": "1.0.0",
  "description": "",
  "defines": {
    "sdkPath": "app/components/service/",
    "testCommand": "nyc --reporter=html mocha --timeout 5000 --require mock-local-storage --reporter spec"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/sass -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass -o src/styles/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js build-widgets",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "documentTutorial": "jsdoc  -u src/tutorial -c jsdoc.json",
    "documentTutorial-windows": "jsdoc -c jsdoc.json -u %npm_package_defines_appPath%/tutorial",
    "build:clean-windows": "cd build && mkdir ..\\value-x\\v%npm_package_version%-beta\\static && move static\\js\\*.js ..\\value-x\\v%npm_package_version%-beta\\static\\sdk.js && move static\\css\\*.css ..\\value-x\\v%npm_package_version%-beta\\static\\sdk.css",
    "build:clean": "cd build && mkdir -p ../value-x/v$npm_package_version-beta && cp static/js/*.js ../value-x/v$npm_package_version-beta/static/sdk.js && cp static/css/*.css ../value-x/v$npm_package_version-beta/static/sdk.css",
    "build-sdk-w": "copy src\\index.js src\\index.default.js && copy src\\index.sdk.js src\\index.js && npm run build-sdk1-w && copy  src\\index.default.js src\\index.js && del src\\index.default.js",
    "build-sdk1-w": "npm-run-all build-css build-js build-sdk2-w",
    "build-sdk2-w": "xcopy build /e valuex-sdk\\v%npm_package_version%",
    "build-sdk": "mkdir -p valuex-demo-app/v$npm_package_version && npm run bsdk1",
    "bsdk1": "cp src/index.js src/index.default.js && cp src/index.sdk.js src/index.js && npm run bsdk2",
    "bsdk2": "mkdir -p valuex-demo-app/v$npm_package_version/valuex-sdk && npm-run-all build-css build-js bsdk3",
    "bsdk3": "cp -R build/* valuex-demo-app/v$npm_package_version/value-x-sdk/ && npm run bsdk4",
    "bsdk4": "cp src/index.default.js src/index.js && rm src/index.default.js && npm run rename-sdk-file",
    "rename-sdk-file": "cd valuex-demo-app/v$npm_package_version/value-x-sdk/static/js/ && mv main*.js sdk-bundle.js",
    "build-widgets": "mkdir -p valuex-demo-app/v$npm_package_version && npm run bw1",
    "bw1": "mkdir -p valuex-demo-app/v$npm_package_version/valuex-widgets && npm run bw2",
    "bw2": "cp -R build/* valuex-demo-app/v$npm_package_version/valuex-widgets/ && npm run bw3",
    "bw3": "npm-run-all rename-widgetsJs-file",
    "rename-widgetsJs-file": "pwd && mv valuex-demo-app/v$npm_package_version/valuex-widgets/static/js/main*.js valuex-demo-app/v$npm_package_version/valuex-widgets/static/js/valuex-bundle.js"
  },
  "author": "",
  "license": "ISC",
